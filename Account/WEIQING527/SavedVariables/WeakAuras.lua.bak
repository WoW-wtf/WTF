["customText"] = "function()\n    local item = aura_env.item\n    \n    -- completion coloring\n    local color = aura_env.isComplete() and \"|cFFaad372\" or \"|cFFffffff\"\n    \n    -- fish count\n    local count = aura_env.config.weekly.crCount and item.Count .. \"/20\" or \"\"\n    \n    -- output\n    return color .. item.Name, color .. count\nend",
