["custom"] = "function(states, event, keybind, arg)\n    local cfg = aura_env.config\n    if event == \"ACHIEVEMENT_EARNED\" then\n        if keybind == 41038 then\n            aura_env.collector = select(3, GetAchievementCriteriaInfo(41038, 1)) --double check lol\n        end\n    elseif cfg.mod and event == \"MODIFIER_STATE_CHANGED\" and keybind == aura_env.key and arg == 1 then\n        aura_env.show = not aura_env.show\n        if not aura_env.show then\n            for _, state in pairs(states) do\n                state.show = false\n                state.changed = true\n            end\n            return true\n        end\n    end\n    \n    if aura_env.show and cfg.copy\n    and event == \"GLOBAL_MOUSE_DOWN\" and keybind and not InCombatLockdown()\n    then\n        local button = keybind\n        if (button == \"LeftButton\" or button == \"RightButton\") and IsLeftControlKeyDown() then\n            local questID\n            for _, v in pairs(aura_env.regions) do\n                local over = MouseIsOver(v)\n                if over then\n                    questID = v.state and v.state.popup\n                    break\n                end\n            end\n            if questID then\n                StaticPopup_Show(\"TRANSLIT_20TH\", questID, C_QuestLog.GetTitleForQuestID(questID))\n            end\n        end\n    elseif event == \"OPTIONS\"\n    or (not cfg.mod and event ~= \"MODIFIER_STATE_CHANGED\")\n    or (cfg.mod and aura_env.show and (event == \"MODIFIER_STATE_CHANGED\" and arg == 1 or event ~= \"MODIFIER_STATE_CHANGED\")) then \n        if cfg.mod and aura_env.show and event == \"MODIFIER_STATE_CHANGED\" and arg == 1 then\n            aura_env.requestData()\n        end\n        local count = 1\n        --screw that\n        for _, state in pairs(states) do\n            state.show = false\n            state.changed = true\n        end\n        \n        for i, data in ipairs(aura_env.data) do\n            if data.enabled and i ~= 8 then\n                local npc = data.npc\n                states[npc] = {\n                    hub = i,\n                    index = count,\n                    title = npc,\n                    changed = true,\n                    show = true,\n                }\n                count = count + 1\n                \n                local questID, questAtlas, showNpc, progress\n                for _, questInfo in ipairs(data.quests) do\n                    progress, questAtlas = questInfo[1], questInfo[2]\n                    if not progress then\n                        local title = questInfo[3]\n                        local completed = aura_env.instance()\n                        local show = not cfg.hide or cfg.hide and not completed\n                        showNpc = showNpc or show\n                        \n                        states[title] = {\n                            hub = i,\n                            index = count,\n                            title = format(\"%s %s\", questAtlas, title),\n                            completed = completed,\n                            changed = true,\n                            show = show,\n                        }\n                        count = count + 1\n                    elseif type(progress) == \"table\" then\n                        local completed, completedID, IsOnQuest, IsOnQuestID\n                        for _, id in ipairs(progress) do\n                            completed = completed or C_QuestLog.IsQuestFlaggedCompletedOnAccount(id)\n                            \n                            if completed and not completedID then\n                                completedID = id\n                            end  \n                            IsOnQuest = IsOnQuest or C_QuestLog.IsOnQuest(id)\n                            if IsOnQuest and not IsOnQuestID then\n                                IsOnQuestID = id\n                            end\n                        end\n                        \n                        questID = IsOnQuestID or completedID\n                        if questID == 82449 and completed then --The Call of the Worldsoul\n                            completed = C_QuestLog.IsQuestFlaggedCompleted(questID)\n                        end\n                        local title = questID and C_QuestLog.GetTitleForQuestID(questID) or questInfo[3]\n                        local show = not cfg.hide or cfg.hide and not completed\n                        showNpc = showNpc or show\n                        \n                        states[title] = {\n                            hub = i,\n                            index = count,\n                            title = format(\"%s %s\", questAtlas, title),\n                            completed = completed,\n                            changed = true,\n                            show = show,\n                            popup = questID,\n                        }\n                        count = count + 1\n                        \n                        local questData = IsOnQuestID and C_QuestLog.GetQuestObjectives(IsOnQuestID)\n                        if questData and questData[1] then\n                            for j, v in ipairs(questData) do\n                                local key = IsOnQuestID..j\n                                states[key] = {\n                                    hub = i,\n                                    questID = IsOnQuestID,\n                                    index = count,\n                                    step = \"• \",\n                                    IsOnQuest = IsOnQuest,\n                                    text = v.text,\n                                    finished = v.finished,\n                                    fulfilled = v.numFulfilled,\n                                    required = v.numRequired,\n                                    changed = true,\n                                    show = show,\n                                }\n                                count = count + 1\n                            end\n                        end\n                    else\n                        questID = progress\n                        local completed = C_QuestLog.IsQuestFlaggedCompletedOnAccount(questID)\n                        \n                        if questID == 84616 and not completed then --Celebrate Good Fun! workaround\n                            completed = not aura_env.getReward(questID)\n                        end\n                        \n                        local show = not cfg.hide or cfg.hide and not completed\n                        if questInfo[4] and completed then --hide completed/not awarding PvP quests (until realizing how to check for current weekly rotation)\n                            show = false\n                        end\n                        showNpc = showNpc or show\n                        \n                        local title = questInfo[3] or C_QuestLog.GetTitleForQuestID(questID) or questID\n                        states[title] = {\n                            hub = i,\n                            index = count,\n                            title = format(\"%s %s\", questAtlas or \"??\", title),\n                            completed = completed,\n                            changed = true,\n                            show = show,\n                            popup = questID,\n                        }\n                        count = count + 1\n                        \n                        local IsOnQuest = C_QuestLog.IsOnQuest(questID)\n                        local questData = C_QuestLog.GetQuestObjectives(questID)\n                        if questData and questData[1] then\n                            for j, v in ipairs(questData) do\n                                local key = questID..j\n                                if states[key] and not IsOnQuest then\n                                    states[key].changed = true\n                                    states[key].show = false \n                                elseif IsOnQuest then\n                                    states[key] = {\n                                        hub = i,\n                                        questID = questID,\n                                        index = count,\n                                        step = \"• \",\n                                        IsOnQuest = IsOnQuest,\n                                        text = v.text,\n                                        finished = v.finished,\n                                        fulfilled = v.numFulfilled,\n                                        required = v.numRequired,\n                                        changed = true,\n                                        show = show,\n                                    }\n                                    count = count + 1\n                                end\n                            end\n                        end\n                    end\n                end\n                count = count + 50\n                if not showNpc and states[npc] then\n                    states[npc].show = false\n                    states[npc].changed = true\n                end\n            end\n        end\n        --\n        if aura_env.collector and cfg.daily then\n            local data = aura_env.data[8]\n            local npc, showNpc\n            if data then\n                npc = data.npc\n                states[npc] = {\n                    hub = i,\n                    index = count,\n                    title = npc,\n                    changed = true,\n                    show = true,\n                }\n                count = count + 1\n                \n                for _, questInfo in ipairs(data.quests) do\n                    local questID\n                    local progress, questAtlas, title, map, poi = questInfo[1], questInfo[2], questInfo[3], questInfo[4], questInfo[5]\n                    \n                    if type(progress) == \"table\" then \n                        if  cfg.pvp then\n                            local completed, completedID\n                            for _, id in ipairs(progress) do\n                                completed = C_QuestLog.IsQuestFlaggedCompleted(id)\n                                if completed then\n                                    completed = true\n                                    break\n                                end  \n                            end\n                            \n                            local show = not completed \n                            showNpc = showNpc or show\n                            \n                            states[title] = {\n                                index = count,\n                                title = format(\"%s %s\", questAtlas, title),\n                                completed = false,\n                                changed = true,\n                                show = show,\n                                popup = UnitFactionGroup(\"player\") == \"Alliance\" and progress[1] or progress[2]\n                            }\n                            count = count + 1\n                        end\n                    else\n                        questID = progress\n                        local onMap = map and poi and C_AreaPoiInfo.GetAreaPOIInfo(map, poi)\n                        local completed = questID and C_QuestLog.IsQuestFlaggedCompleted(questID)\n                        if onMap or completed == false then\n                            showNpc = true\n                            \n                            states[title] = {\n                                index = count,\n                                title = format(\"%s %s\", questAtlas or \"??\", title),\n                                completed = false,\n                                changed = true,\n                                show = true,\n                            }\n                            count = count + 1\n                        end\n                    end\n                end\n            end\n            count = count + 50\n            \n            for _, difficulty in ipairs({17,14,15}) do --LFR, Normal, Heroic\n                states[difficulty] = {\n                    index = count,\n                    title = format(\"%s (%s)\", EJ_GetInstanceInfo(1301) or \"BRD\", GetDifficultyInfo(difficulty)) or difficulty,\n                    completed = false,\n                    changed = true,\n                    show = true,\n                    tooltip = \"\",\n                }\n                count = count + 1\n                \n                local tooltip = \"\"\n                local show = false\n                for i, data in ipairs(aura_env.brd) do\n                    local encounterID, questAtlas, encounterName, miniboss = data[1], data[2], data[3], data[4]\n                    \n                    if not miniboss then\n                        local completed = C_RaidLocks.IsEncounterComplete(2792, encounterID, difficulty)\n                        show = show or not completed\n                        showNpc = showNpc or show\n                        \n                        tooltip = format(\"%s%s%s |cFF%s%s|r\",\n                            tooltip,\n                            tooltip == \"\" and \"\" or \"|n\",\n                            questAtlas,\n                            completed and \"00FF00\" or \"FF0000\",\n                            encounterName\n                        )\n                    elseif type(encounterID) == \"table\" and encounterID[difficulty] then\n                        local completed = C_QuestLog.IsQuestFlaggedCompleted(encounterID[difficulty])\n                        show = show or not completed\n                        showNpc = showNpc or show\n                        \n                        tooltip = format(\"%s%s%s |cFF%s%s|r\",\n                            tooltip,\n                            tooltip == \"\" and \"\" or \"|n\",\n                            questAtlas,\n                            completed and \"00FF00\" or \"FF0000\",\n                            encounterName\n                        )\n                    end\n                    \n                    if show then\n                        states[difficulty].tooltip = tooltip\n                    end\n                    states[difficulty].show = show\n                end\n            end\n            \n            if not showNpc and states[npc] then\n                states[npc].show = false\n                states[npc].changed = true\n            end\n        end\n        \n        return true\n    end\nend",
