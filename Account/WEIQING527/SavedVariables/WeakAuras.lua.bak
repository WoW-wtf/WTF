["init"] = {
["custom"] = "aura_env.regions = {}\n\naura_env.db = {\n    ----------------------------------------------------------------\n    -- Adventurer's Cache\n    ----------------------------------------------------------------\n    [\"adventureCache\"] = {\n        [\"name\"] = {\n            [\"enUS\"] = \"Adventurer's Cache\",          \n            [\"zhCN\"] = \"冒险家的宝箱\",\n            [\"zhTW\"] = \"冒險家的寶箱\",\n        },\n        \n        [\"isWeekend\"] = false,\n        [\"questReq\"] = 4,\n        [\"questID\"] = { 84736, 84737, 84738, 84739 },\n    },\n    \n    ----------------------------------------------------------------\n    -- weeklyQuest\n    ----------------------------------------------------------------\n    [\"weeklyQuest\"] = {\n        [\"name\"] = {\n            --[\"enUS\"] = \"\",          \n            [\"zhCN\"] = \"文物管理员周任\",\n            [\"zhTW\"] = \"文物管理員週任\",\n        },\n        \n        [\"isWeekend\"] = false,\n        [\"questReq\"] = 1,\n        [\"questID\"] = { 82678, 83363, 83366, 83345 },\n    },\n    \n    ----------------------------------------------------------------\n    -- Awaken the Machine\n    ----------------------------------------------------------------\n    [\"awakenMachine\"] = {\n        [\"name\"] = {\n            [\"enUS\"] = \"Awaken the Machine\",\n            [\"zhCN\"] = \"主机觉醒\",\n            [\"zhTW\"] = \"甦醒機械\",\n        },\n        \n        [\"isWeekend\"] = false,\n        [\"questReq\"] = 1,\n        [\"questID\"] = { 83333 },\n    },\n    \n    ----------------------------------------------------------------\n    -- Hallowfall Fishing Derby\n    ----------------------------------------------------------------\n    [\"fishingDerby\"] = {\n        [\"name\"] = {\n            [\"enUS\"] = \"Hallowfall Fishing Derby\",\n            [\"zhCN\"] = \"陨圣峪钓鱼大赛\",\n            [\"zhTW\"] = \"聖落之地釣魚大賽\",\n        },\n        \n        [\"isWeekend\"] = true,\n        [\"questReq\"] = 1,\n        [\"questID\"] = { 83529, 83530, 83531, 83532, 82778 },\n    },\n    \n    ----------------------------------------------------------------\n    -- World Soul\n    ----------------------------------------------------------------\n    [\"worldSoul\"] = {\n        [\"name\"] = {\n            [\"enUS\"] = \"World Soul\",\n            [\"zhCN\"] = \"世界之魂\",\n            [\"zhTW\"] = \"世界之魂\",\n        },\n        \n        [\"isWeekend\"] = false,\n        [\"questReq\"] = 1,\n        [\"questID\"] = {\n            82452, 82483, 82659, 82482, 82511, 82489, 82495, 82493, 82516, 82490, 82512,\n            82510, 82485, 82491, 82509, 82453, 82501, 82486, 82494, 82502, 82500, 82487,\n            82488, 82492, 82499, 82506, 82503, 82508, 82504, 82505, 82507, 82497, 82496, 82498\n        },\n    },\n    \n    ----------------------------------------------------------------\n    -- Rollin' Down in the Deeps\n    ----------------------------------------------------------------\n    [\"rollinDeep\"] = {\n        [\"name\"] = {\n            [\"enUS\"] = \"Rollin' Down in the Deeps\",\n            [\"zhCN\"] = \"滚滚深邃都是蜡\",\n            [\"zhTW\"] = \"滾入深處\",\n        },\n        \n        [\"isWeekend\"] = false,\n        [\"questReq\"] = 1,\n        [\"questID\"] = { 82946 },\n    },\n    \n    ----------------------------------------------------------------\n    -- Spreading The Light\n    ----------------------------------------------------------------\n    [\"spreadLight\"] = {\n        [\"name\"] = {\n            [\"enUS\"] = \"Spreading The Light\",\n            [\"zhCN\"] = \"散布圣光(陨圣峪)\",\n            [\"zhTW\"] = \"散布光芒(聖落之地)\",\n        },\n        \n        [\"isWeekend\"] = false,\n        [\"questReq\"] = 1,\n        [\"questID\"] = { 76586, 82050 },\n    },\n    \n    ----------------------------------------------------------------\n    -- Theater Troupe\n    ----------------------------------------------------------------\n    [\"theaterTroupe\"] = {\n        [\"name\"] = {\n     